<!-- start prev next wrap -->
<div class="prev-next-wrap clearfix">
	<div class="author-specs">
		{{#next_post}}
		<a class="btn btn-default" style="float:left;" href="{{url}}"><i class="fa fa-angle-left fa-fw"></i> {{title}}</a>
		{{/next_post}}
    {{#prev_post}}
        <a class="btn btn-default" style="float:right;" href="{{url}}">{{title}} <i class="fa fa-angle-right fa-fw"></i></a>
    {{/prev_post}}
		</div>
</div>
